local SpeedButton = Instance.new("TextButton")
SpeedButton.Size = UDim2.new(0.6, 0, 0.15, 0)
SpeedButton.Position = UDim2.new(0, 0, 0.90, 0)
SpeedButton.Text = "Speed"
SpeedButton.TextColor3 = Color3.new(0.8, 0.3, 0.9)
