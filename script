SpeedButton.TextSize = 16
SpeedButton.BackgroundColor3 = Color3.new(0.1, 0, 0.2)
SpeedButton.Parent = Frame

local SpeedInput = Instance.new("TextBox")
SpeedInput.Size = UDim2.new(0.4, 0, 0.15, 0)
SpeedInput.Position = UDim2.new(0.6, 0, 0.90, 0)
SpeedInput.Text = "16"
SpeedInput.TextColor3 = Color3.new(1, 1, 1)
SpeedInput.Font = Enum.Font.Gotham
SpeedInput.TextSize = 16
SpeedInput.BackgroundColor3 = Color3.new(0.2, 0, 0.3)
SpeedInput.Parent = Frame

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local hrp = character:WaitForChild("HumanoidRootPart")

local flying = false
local bodyGyro
local bodyVelocity
local flySpeed = 50

local function setNoCollisions(enable)
    for _, part in ipairs(character:GetDescendants()) do
        if part:IsA("BasePart") and part.CanCollide ~= enable then
            part.CanCollide = enable
        end
    end
end

local function enableFly()
    if flying then return end
    flying = true
    humanoid.PlatformStand = true
    setNoCollisions(false)

    bodyVelocity = Instance.new("BodyVelocity", hrp)
    bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    bodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000)

    bodyGyro = Instance.new("BodyGyro", hrp)
    bodyGyro.MaxTorque = Vector3.new(400000, 400000, 400000)
    bodyGyro.CFrame = hrp.CFrame
end

local function disableFly()
    flying = false
    humanoid.PlatformStand = false
    setNoCollisions(true)

    if bodyVelocity then bodyVelocity:Destroy() end
    if bodyGyro then bodyGyro:Destroy() end
end

FlyButton.MouseButton1Click:Connect(function()
    if flying then
        disableFly()
        FlyButton.Text = "Fly"
    else
        enableFly()
        FlyButton.Text = "Disable Fly"
    end
end)

local userInput = game:GetService("UserInputService")
game:GetService("RunService").RenderStepped:Connect(function()
    if flying then
        local direction = Vector3.new()

        if userInput:IsKeyDown(Enum.KeyCode.W) then
            direction = direction + hrp.CFrame.LookVector
        end
        if userInput:IsKeyDown(Enum.KeyCode.S) then
            direction = direction - hrp.CFrame.LookVector
        end
        if userInput:IsKeyDown(Enum.KeyCode.A) then
            direction = direction - hrp.CFrame.RightVector
        end
        if userInput:IsKeyDown(Enum.KeyCode.D) then
            direction = direction + hrp.CFrame.RightVector
        end
        if userInput:IsKeyDown(Enum.KeyCode.Space) then
            direction = direction + Vector3.new(0, 1, 0)
        end
        if userInput:IsKeyDown(Enum.KeyCode.LeftControl) then
            direction = direction - Vector3.new(0, 1, 0)
        end

        bodyVelocity.Velocity = direction * flySpeed
        bodyGyro.CFrame = CFrame.new(hrp.Position, hrp.Position + hrp.CFrame.LookVector)
    end
end)

local menuVisible = true
ToggleMenuButton.MouseButton1Click:Connect(function()
    menuVisible = not menuVisible
    Frame.Visible = menuVisible
    ToggleMenuButton.Text = menuVisible and "Hide Menu" or "Show Menu"
    ShowMenuButton.Visible = not menuVisible
end)

ShowMenuButton.MouseButton1Click:Connect(function()
    menuVisible = true
    Frame.Visible = true
    ShowMenuButton.Visible = false
    ToggleMenuButton.Text = "Hide Menu"
end)

local noclip = false
NoclipButton.MouseButton1Click:Connect(function()
    noclip = not noclip
    NoclipButton.Text = noclip and "Disable Noclip" or "Noclip"
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = not noclip
        end
    end
end)

local infiniteJump = false
InfiniteJumpButton.MouseButton1Click:Connect(function()
    infiniteJump = not infiniteJump
    InfiniteJumpButton.Text = infiniteJump and "Disable Infinite Jump" or "Infinite Jump"
    if infiniteJump then
        game:GetService("UserInputService").JumpRequest:Connect(function()
            if infiniteJump then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    end
end)

local infiniteHealth = false
InfiniteHealthButton.MouseButton1Click:Connect(function()
    infiniteHealth = not infiniteHealth
    InfiniteHealthButton.Text = infiniteHealth and "Disable Infinite Health" or "Infinite Health"
    if infiniteHealth then
        humanoid.HealthChanged:Connect(function()
            if infiniteHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end)

        humanoid.Died:Connect(function()
            if infiniteHealth then
                humanoid.Health = humanoid.MaxHealth
                humanoid:ChangeState(Enum.HumanoidStateType.Physics)
            end
        end)
    end
end)

local speedValue = 16
SpeedButton.MouseButton1Click:Connect(function()
    local speedInputValue = tonumber(SpeedInput.Text)
    if speedInputValue and speedInputValue > 0 then
        speedValue = speedInputValue
    else
        SpeedInput.Text = "16"
    end
end)

local function setSpeed()
    humanoid.WalkSpeed = speedValue
end
game:GetService("RunService").Heartbeat:Connect(setSpeed)
