local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui
ScreenGui.ResetOnSpawn = false

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 250, 0, 360)
Frame.Position = UDim2.new(0.5, -125, 0.5, -180)
Frame.BackgroundColor3 = Color3.new(0.05, 0, 0.1)
Frame.Parent = ScreenGui
Frame.Active = true
Frame.Draggable = true

local DeveloperLabel = Instance.new("TextLabel")
DeveloperLabel.Text = "Developer: VTLSEV"
DeveloperLabel.Size = UDim2.new(1, 0, 0.15, 0)
DeveloperLabel.Position = UDim2.new(0, 0, 0, 0)
DeveloperLabel.TextColor3 = Color3.new(0.8, 0.3, 0.9)
DeveloperLabel.Font = Enum.Font.Gotham
DeveloperLabel.TextSize = 14
DeveloperLabel.BackgroundTransparency = 1
DeveloperLabel.Parent = Frame

local FlyButton = Instance.new("TextButton")
FlyButton.Size = UDim2.new(1, 0, 0.15, 0)
FlyButton.Position = UDim2.new(0, 0, 0.15, 0)
